<table class="table table-hover table-bordered results">
  <thead>
    <tr>
      <th class="col-md-2 col-xs-2">Product-Id</th>
      <th class="col-md-5 col-xs-5">Product Name</th>
      <th class="col-md-3 col-xs-3">Quantity</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      {{#qty}}
      <td>{{product_id}}
      <td>{{product_name}}</td>
      <td>{{quantity}}</td>
      </tr>
      {{/qty}}
  </tbody>
</table>
